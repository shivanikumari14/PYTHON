#read the data
 f = open("demo.txt" , "r")
 data = f.read()
 print(data)
 print(type(data))
 f.close()

#read the data (pass paramter to read)
 f = open("demo.txt" , "r")
 data = f.read(5)
 print(data)
 f.close()

#readline
f = open("demo.txt" , "r")
line1 = f.readline()
print(line1)
f.close()

# wite and append the data
f = open("demo.txt","w")
f.write("I am a student of ECE. ")
f.close()

f = open("demo.txt","a")
f.write("\nI am a  good girl. ")
f.close()

